{
  "name": "mobile",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/mobile",
  "projectType": "application",
  "tags": ["type:app", "scope:mobile"],
  "targets": {
    "start": {
      "executor": "@nx/expo:start",
      "options": {}
    },
    "run-ios": {
      "executor": "@nx/expo:run",
      "options": {
        "platform": "ios"
      }
    },
    "run-android": {
      "executor": "@nx/expo:run",
      "options": {
        "platform": "android"
      }
    },
    "build": {
      "executor": "@nx/expo:export",
      "outputs": ["{options.outputDir}"],
      "options": {
        "platform": "all",
        "outputDir": "dist/apps/mobile"
      }
    },
    "build:eas": {
      "executor": "@nx/expo:build",
      "options": {},
      "configurations": {
        "local": {
          "local": true
        }
      }
    },
    "export": {
      "executor": "@nx/expo:export",
      "outputs": ["{options.outputDir}"],
      "options": {
        "platform": "all",
        "outputDir": "dist/apps/mobile"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {}
    }
  }
}
